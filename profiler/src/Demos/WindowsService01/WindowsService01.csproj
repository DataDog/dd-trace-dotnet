    <ProjectGuid>{A2FCEEF7-975E-495C-AD0D-6A79176B3A44}</ProjectGuid>
    <OutputType>WinExe</OutputType>
    <RootNamespace>Datadog.Demos.WindowsService01</RootNamespace>
