@using System.Diagnostics;
@using System.Security;

@using Datadog.TestUtil;

@using Website_AspNet
@using Website_AspNet.Models

@model IndexModel
@{
    ViewBag.Title = "Home Page";
}

<div class="text-left">

    <div class="container">
        <div><h1 class="display-4">Computation result</h1></div>
        <div>
            <p>
                Computation of Fibonacci number @Model.ComputationData.Input took @Model.ComputationData.Duration and the result is @Model.ComputationData.Result.
            </p>
        </div>
    </div>
    <div class="container">
        <div><h1 class="display-4">Environment info:</h1></div>
        <div><pre>@(EnvironmentInfoPublic.GetDescription())</pre></div>
    </div>

</div>
