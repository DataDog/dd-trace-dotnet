<Project>
  <Import Project="..\Directory.Build.props" />

  <PropertyGroup>
    <!-- overrides -->
    <IsPackable>true</IsPackable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>

    <!-- NuGet package -->
    <Authors>Datadog</Authors>
    <PackageIconUrl></PackageIconUrl>
    <PackageProjectUrl>https://github.com/DataDog/dd-trace-csharp</PackageProjectUrl>
    <!-- Use the python license's url while the project's github is not public -->
    <PackageLicenseUrl>https://github.com/DataDog/dd-trace-py/blob/master/LICENSE</PackageLicenseUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>https://github.com/DataDog/dd-trace-csharp/releases</PackageReleaseNotes>
    <PackageTags>Datadog APM tracing profiling</PackageTags>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/DataDog/dd-trace-csharp.git</RepositoryUrl>
    <Copyright>Datadog, Inc. 2017-2018</Copyright>
  </PropertyGroup>

  <!-- StyleCop -->
  <ItemGroup>
    <Compile Include="..\GlobalSuppressions.cs" Link="GlobalSuppressions.src.cs" />
  </ItemGroup>
</Project>
