<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45</TargetFrameworks>

    <!-- NuGet -->
    <Version>1.11.1-prerelease</Version>
    <Title>Datadog APM Tracing for ASP.NET</Title>
    <Description>
      OBSOLETE. This package is deprecated and exists only for backwards compatibility.
      ASP.NET is now automatically instrumented. If your project references this package 
      or Datadog.Trace.AspNet, you can remove them.
    </Description>

    <!-- Allow the GenerateAssemblyInfo task in the .NET SDK to generate all
         assembly attributes except for the AssemblyVersionAttribute. This will
         be locked at Major.0.0.0 and is defined in AssemblyInfo.cs -->
    <GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>

    <IncludeBuildOutput>false</IncludeBuildOutput>
    <NoWarn>$(NoWarn);NU5128</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Datadog.Trace\Datadog.Trace.csproj" />

    <Reference Include="System.Net.Http" />
    <Reference Include="System.Web" />
  </ItemGroup>

</Project>