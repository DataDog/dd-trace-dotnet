<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Company>Datadog Ltd.</Company>
    <Copyright>Datadog Ltd.</Copyright>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <LangVersion>latest</LangVersion>
    <PackageIcon>icon.ico</PackageIcon>
    <PackageIconUrl />
    <RunAnalyzersDuringBuild>False</RunAnalyzersDuringBuild>
    <AnalysisLevel>none</AnalysisLevel>
    <RunAnalyzersDuringLiveAnalysis>False</RunAnalyzersDuringLiveAnalysis>
    <ApplicationIcon>icon.ico</ApplicationIcon>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;DEBUG</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\InstrumentedAssemblyGenerator\Datadog.InstrumentedAssemblyGenerator.csproj" />
    <ProjectReference Include="..\InstrumentedAssemblyVerification\Datadog.InstrumentedAssemblyVerification.csproj" />
  </ItemGroup>

</Project>
