{
  "dsl": "any(ref CustomArrayLocal, { @it.NestedString == 'hello' })",
  "any": [
    {
      "ref": "CustomArrayLocal"
    },
    {
      "eq": [
        {
          "getmember":[
            {
              "ref": "@it"
            },
            "NestedString"
          ] 
        },
        "hello"
      ]
    }
  ]
}