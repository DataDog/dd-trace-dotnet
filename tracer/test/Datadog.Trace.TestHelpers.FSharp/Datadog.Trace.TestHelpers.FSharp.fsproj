<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <LangVersion>5.0</LangVersion>
    <!-- When doing a nuget restore on Datadog.Trace.sln through the dotnet SDK, nuget claims there's a package downgrade from System.ValueTuple 4.4.0 to 4.5.0. Just ignore the warning, that shouldn't be happening. -->
    <NoWarn>NU1605</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Datadog.Trace.TestHelpers\Datadog.Trace.TestHelpers.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="ValidationTypes.fs" />
    <Compile Include="SpanModelHelpers.fs" />
    <Compile Include="SpanModel.fs" />
    <Compile Include="SpanValidator.fs" />
    <Compile Include="Library.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>

</Project>
