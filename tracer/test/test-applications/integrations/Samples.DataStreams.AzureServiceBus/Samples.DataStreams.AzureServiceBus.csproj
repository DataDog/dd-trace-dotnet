  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\Samples.AzureServiceBus\CustomTracerProviderBuilderExtensions.cs">
      <Link>CustomTracerProviderBuilderExtensions.cs</Link>
    </Compile>
  </ItemGroup>
