<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net8.0;net9.0</TargetFrameworks>
    <!-- System.Runtime.CompilerServices.Unsafe doesn't support netcoreapp2.1-->
    <!-- https://andrewlock.net/stop-lying-about-netstandard-2-support/-->
    <SuppressTfmSupportBuildWarnings>true</SuppressTfmSupportBuildWarnings>
    <ApiVersion Condition="'$(ApiVersion)' == ''">1.7.0</ApiVersion>
    <DefineConstants Condition="'$(ApiVersion)'&gt;='1.2.0'">$(DefineConstants);OTEL_1_2</DefineConstants>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Quartz" Version="3.15.0" />
  </ItemGroup>

  
  
</Project>