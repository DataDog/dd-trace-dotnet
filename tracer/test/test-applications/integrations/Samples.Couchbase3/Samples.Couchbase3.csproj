<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <ApiVersion Condition="'$(ApiVersion)' == ''">3.2.3</ApiVersion>
    <DefineConstants Condition="'$(ApiVersion)'&gt;='3.1.0'">$(DefineConstants);COUCHBASE_3_0_0</DefineConstants>
    <OutputType>Exe</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CouchbaseNetClient" Version="$(ApiVersion)" />
  </ItemGroup>

</Project>