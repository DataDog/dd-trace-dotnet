{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Trace",
      "Host.*": "Trace",
      "Microsoft.*": "Trace"
    },
    "console": {
      "isEnabled": true
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }
}